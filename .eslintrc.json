{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "alloy",
        "alloy/typescript",
        "alloy/vue",
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {}
}