{
    "files.eol": "\n",
    "editor.tabSize": 4,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "eslint.validate": [
        "javascript",
        "vue",
        "typescript"
    ],
    "vetur.format.enable": false,
    "vetur.validation.template": false,
    "typescript.tsdk": "node_modules/typescript/lib",
    "prettier.disableLanguages": [],
    "eslint.options": {
        "extensions": [
            ".js",
            ".ts",
            ".vue",
            ".json"
        ]
    }
}